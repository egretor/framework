<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="struts-manage" extends="struts-secutiry"
		namespace="/manage">
		<action name="user!*" class="unknown.website.manage.application.ManageUserAction">
			<result name="success">/manage/user/index.jsp</result>
		</action>
		
		<action name="catalog!*" class="unknown.website.manage.application.ManageCatalogAction">
			<result name="success">/manage/user/index.jsp</result>
		</action>
	</package>
</struts>

